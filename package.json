{
  "name": "open-mqtt-gateway-editor",
  "productName": "open-mqtt-gateway-editor",
  "version": "1.0.0",
  "description": "Editor for Open MQTT Gateway project",
  "main": "./main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "Linh",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "download-git-repo": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^8.0.1",
    "ini": "^1.3.5",
    "jquery": "^3.4.1",
    "lodash.debounce": "^4.0.8",
    "mqtt": "^3.0.0",
    "node-cmd": "^3.0.0",
    "serialport": "^7.1.5",
    "node-pty": "^0.8.0",
    "xterm": "^3.14.2",
    "xterm-addon-ligatures": "0.1.0-beta-1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.39",
    "@electron-forge/maker-deb": "^6.0.0-beta.39",
    "@electron-forge/maker-rpm": "^6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.39",
    "@electron-forge/maker-zip": "^6.0.0-beta.39",
    "electron": "4.0.1",
    "electron-rebuild": "^1.8.5",
    "literally-html": "^0.1.1"
  }
}
